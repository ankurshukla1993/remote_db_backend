<p id="notice"><%= notice %></p>

<h1>Sql Queries</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sql_queries.each do |sql_query| %>
      <tr>
        <td><%= link_to 'Show', sql_query %></td>
        <td><%= link_to 'Edit', edit_sql_query_path(sql_query) %></td>
        <td><%= link_to 'Destroy', sql_query, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sql Query', new_sql_query_path %>
